//6 from week3 exercises
#include <iostream>
using namespace std;
int main(){
	int A=512,a=8,b=16,c=32,d=64,C=0;
	for (int i=0;i<(A/a+1);i++){
		for (int j=0;j<(A/b+1);j++){
			for (int t=0;t<(A/c+1);t++){
				for (int z=0;z<(A/d+1);z++){
					if (i*a+j*b+t*c+z*d==A){
						C++;
					}
				}
			}
		}
	}
	cout<<C<<"\n";
	return 0;

}
