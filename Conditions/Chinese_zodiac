#include <iostream>
using namespace std;
int main(){
    int a;
    cout<<"Enter the year:";
    cin>>a;
    if (a<0){
        cout<<"Invalid year!"<<endl;
    }else{
        int b=a%12;
        if (b==1){
            cout<<a<<" is the year of Rooster "<<endl;
        }else if (b==2){
            cout<<a<<" is the year of Dog"<<endl;
        }else if (b==3){
            cout<<a<<" is the year of Pig"<<endl;
        }else if (b==4){
            cout<<a<<" is the year of Rat"<<endl;
        }else if (b==5){
            cout<<a<<" is the year of Ox"<<endl;
        }else if (b==6){
            cout<<a<<" is the year of Tiger"<<endl;
        }else if (b==7){
            cout<<a<<" is the year of Hare"<<endl;
        }else if (b==8){
            cout<<a<<" is the year of Dragon"<<endl;
        }else if (b==9){
            cout<<a<<" is the year of Snake"<<endl;
        }else if (b==10){
            cout<<a<<" is the year of Horse"<<endl;
        }else if (b==11){
            cout<<a<<" is the year of Sheep"<<endl;
        }else{
            cout<<a<<" is the year of Monkey"<<endl;
        }
    }
}
