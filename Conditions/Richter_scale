//Richter scale
#include <iostream>
using namespace std;
int main(){
    float x;
    cin>>x;
    if (x<2){
        cout<<"Micro"<<endl;
    }else if (2<=x<3){
        cout<<"Very minor"<<endl;
    }else if (3<=x<4){
        cout<<"Minor"<<endl;
    }else if (4<=x<5){
        cout<<"Light"<<endl;
    }else if (5<=x<6){
        cout<<"Moderate"<<endl;
    }else if (6<=x<7){
        cout<<"Strong"<<endl;
    }else if (7<=x<8){
        cout<<"Major"<<endl;
    }else if (8<=x<10){
        cout<<"Great"<<endl;
    }else {
        cout<<"Meteoric"<<endl;
    }
    return 0;
}
