#include <iostream>
#include <string>
using namespace std;
string myFunc(int x,int y){
    string flag="False";
    if ((x/y)%2==0){
        flag="True";
    }
    return flag;
}

int main(){
    int a,b;
    while (true){
        cin>>a>>b;
        if (a>b){
            break;
        }
    }
    cout<<myFunc(a,b)<<endl;
    return 0;
}
