#include <iostream>
#include <cmath>
#include <cstdio>
using namespace std;
int countDigit(int x ){
    int y=abs(x),c=0;
    while (y>0){
        c++;
        y/=10;
    }
    return c;
}

int main(){
    int n;
    cin>>n;
    printf("The length of %d is %d\n",n,countDigit(n));
    return 0;
    
}
