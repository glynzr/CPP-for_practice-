#include <iostream>
using namespace std;
int isPronic(int x){
    for (int i=1;i<x;i++){
        if (i*(i+1)==x){
            return true;
        }
    }
    return false;
}

int main(){
    int p;
    cin>>p;
    if (isPronic(p)){
        cout<<p<<" is a Pronic number"<<endl;
    }else{
        cout<<p<<" is not a Pronic number"<<endl;
    }
}
